    "def fact(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * fact(n - 1)\n",
    "\n",
    "import sys\n",
    "sys.setrecursionlimit(10**5)    \n",
    "N = int(input())\n",
    "print(fact(N))"
   ]
  }
