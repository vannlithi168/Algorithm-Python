    "def hanoi(n, src, via, dst):\n",
    "    if n == 1:\n",
    "        print(f\"{src} -> {dst}\")\n",
    "    else:\n",
    "        hanoi(n - 1, src, dst, via)\n",
    "        hanoi(1, src, via, dst)\n",
    "        hanoi(n - 1, via, src, dst)\n",
    "\n",
    "N = int(input())\n",
    "hanoi(N, \"A\", \"B\", \"C\")"
   ]
